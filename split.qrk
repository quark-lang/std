{
  (import "list.qrk")
  (let split (fn (string delimitor) {
    (let result (list))
    (let tmp "")
    (foreach string item {
      (if (= item delimitor)
        {
          (push result tmp)
          (set tmp "")
        }
        (set tmp (+ tmp item))
      )
    })
    (if (> (length tmp) 0) (push result tmp))
    (return result)
  }))
}
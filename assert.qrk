{
  (let i 0)
  (let assert (fn (txt el1 el2) {
    (if (= el1 el2) (set i (+ i 1)))
    (print "  " (if (= el1 el2) (green "✓") (red "✕")) (gray txt))
  }))

  (let describe (fn (text tests) {
    (print)
    (print (+ " " text ":"))
    (tests)
  }))

  (let finish (fn () {
    (print)
    (print " " (green (+ "✓ " (+ i " tests complete"))))
    (print)
    (set i 0)
  }))
}